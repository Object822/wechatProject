/* pages/bootPage/bootPage.wxss */
.swiper_wrap {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
}
.boot_img {
    width: 100%;
    height: 100%;
}
.click_in {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 200rpx;
    height: 80rpx;
    color: #fff;
    background: rgba(95, 158, 160, 0.6);
    border-radius: 30rpx;
    bottom: 20%;
    text-align: center;
    line-height: 80rpx;
}